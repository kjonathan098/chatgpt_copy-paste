{
	"manifest_version": 3,
	"name": "chat gpt",
	"version": "1.0",
	"description": "",
	"icons": {
		"128": "./images/128.png",
		"48": "./images/128.png"
	},
	"content_scripts": [
		{
			"matches": ["*://chat.openai.com/chat*"],
			"js": ["content.js"],
			"css": ["style.css"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "clipboardWrite"]
}
